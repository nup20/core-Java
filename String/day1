public class Charat {

	public static void main(String[] args) {
		String s1=new String("Asterisc Computer Institute");
		System.out.println("Original Starting="+(s1));
		System.out.println("The Charecter At the Position 0 is="+(s1.charAt(0)));//Use of charAt();
		System.out.println("The Charecter At the Position 9 is="+(s1.charAt(9)));

	}

}

